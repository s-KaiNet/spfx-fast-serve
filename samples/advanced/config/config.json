{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "bundle-pack-1": {
      "components": [
        {
          "entrypoint": "./lib/webparts/contextInfo/ContextInfoWebPart.js",
          "manifest": "./src/webparts/contextInfo/ContextInfoWebPart.manifest.json"
        },
        {
          "entrypoint": "./lib/webparts/helloWorld/HelloWorldWebPart.js",
          "manifest": "./src/webparts/helloWorld/HelloWorldWebPart.manifest.json"
        }
      ]
    },
    "sample-web-part-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/sampleWebPart/SampleWebPartWebPart.js",
          "manifest": "./src/webparts/sampleWebPart/SampleWebPartWebPart.manifest.json"
        }
      ]
    },
    "my-card-adaptive-card-extension": {
      "components": [
        {
          "entrypoint": "./lib/adaptiveCardExtensions/myCard/MyCardAdaptiveCardExtension.js",
          "manifest": "./src/adaptiveCardExtensions/myCard/MyCardAdaptiveCardExtension.manifest.json"
        }
      ]
    },
    "graph-toolkit-test-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/graphToolkitTest/GraphToolkitTestWebPart.js",
          "manifest": "./src/webparts/graphToolkitTest/GraphToolkitTestWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "marked": "node_modules/marked/marked.min.js"
  },
  "localizedResources": {
    "ContextInfoWebPartStrings": "lib/webparts/contextInfo/loc/{locale}.js",
    "HelloWorldWebPartStrings": "lib/webparts/helloWorld/loc/{locale}.js",
    "SampleWebPartWebPartStrings": "lib/webparts/sampleWebPart/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "MyCardAdaptiveCardExtensionStrings": "lib/adaptiveCardExtensions/myCard/loc/{locale}.js",
    "GraphToolkitTestWebPartStrings": "lib/webparts/graphToolkitTest/loc/{locale}.js"
  }
}
